    "# Data Analysis\n",
    "# Word Count Program\n",
    "def wordCount(filename,word):\n",
    "    with open(filename,\"r\") as f:\n",
    "        if f.mode == \"r\":\n",
    "            x=f.read()\n",
    "            li = x.split()\n",
    "    cnt = li.count(word)        \n",
    "    return cnt\n",
    "filename = input(\"Enter the file name : \")\n",
    "word = input(\"Enter thr word : \")# which word count you neeed\n",
    "wordCount(filename,word)"
   ]
