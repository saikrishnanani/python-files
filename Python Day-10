    "def createFile(filename):\n",
    "    f=open(filename,\"w\")\n",
    "    for i in range(10):\n",
    "        f.write(\"This is %d Line\\n\"%i)\n",
    "    print(\"File is created and data has written\")    \n",
    "    return\n",
    "createFile(\"File1.txt\")"
